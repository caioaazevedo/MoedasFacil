<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.caioalmeida.moedasfacil">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activities.CotacaoActivity">
        </activity>
        <activity android:name=".Activities.ConversaoActivity">
        </activity>
        <activity android:name=".Activities.MoedasActivity">
        </activity>
        <activity android:name=".ConversaoRealActivity">
        </activity>
        <activity android:name=".ConversaoMoedasActivity">
        </activity>
        <activity android:name=".InfoMoedasActivity">
        </activity>
    </application>

</manifest>